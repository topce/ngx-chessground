{
  "name": "ngx-chessground",
  "version": "16.0.3",
  "description": "angular wrapper for ornicar/chessground",
  "scripts": {
    "tsc-lib": "tsc --project ./projects/ngx-chessground/tsconfig.lib.json",
    "tsc-app": "tsc --project ./projects/ngx-chessground-example/tsconfig.app.json",
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "build:lib:prod": "ng build ngx-chessground --configuration production",
    "build:app:prod": "ng build ngx-chessground-example --configuration production --output-path docs --base-href /ngx-chessground/",
    "test": "ng test",
    "lint": "npx rome check ./projects ",
    "lint:fix": "npx rome check ./projects --apply",
    "e2e": "ng e2e",
    "format": "npx rome format ./projects --write ",
    "compodoc": "npx compodoc -p projects/ngx-chessground/tsconfig.doc.json -s -r 9090 -o"
  },
  "dependencies": {
    "@angular/animations": "16.0.3",
    "@angular/cdk": "16.0.2",
    "@angular/common": "16.0.3",
    "@angular/compiler": "16.0.3",
    "@angular/core": "16.0.3",
    "@angular/forms": "16.0.3",
    "@angular/material": "16.0.2",
    "@angular/platform-browser": "16.0.3",
    "@angular/platform-browser-dynamic": "16.0.3",
    "@angular/router": "16.0.3",
    "chess.js": "0.13.4",
    "chessground": "8.3.7",
    "rxjs": "7.8.1",
    "snabbdom": "3.5.1",
    "tslib": "2.5.2",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.0.3",
    "@angular-eslint/builder": "16.0.2",
    "@angular-eslint/eslint-plugin": "16.0.2",
    "@angular-eslint/eslint-plugin-template": "16.0.2",
    "@angular-eslint/schematics": "16.0.2",
    "@angular-eslint/template-parser": "16.0.2",
    "@angular/cli": "16.0.3",
    "@angular/compiler-cli": "16.0.3",
    "@compodoc/compodoc": "1.1.21",
    "@types/chess.js": "0.13.4",
    "@types/jasmine": "4.3.2",
    "@types/node": "20.2.5",
    "jasmine-core": "5.0.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "ng-packagr": "16.0.1",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "protractor": "7.0.0",
    "ts-node": "10.9.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/topce/ngx-chessground.git"
  },
  "keywords": [
    "angular",
    "chess",
    "pgn",
    "fen",
    "chessground",
    "chess.js",
    "lichess"
  ],
  "author": "topce",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/topce/ngx-chessground/issues"
  },
  "homepage": "https://github.com/topce/ngx-chessground#readme"
}