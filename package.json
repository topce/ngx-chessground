{
  "name": "ngx-chessground",
  "version": "12.2.1",
  "description": "angular wrapper for ornicar/chessground",
  "scripts": {
    "tsc-lib": "tsc --project ./projects/ngx-chessground/tsconfig.lib.json",
    "tsc-app": "tsc --project ./projects/ngx-chessground-example/tsconfig.app.json",
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "build:lib:prod": "ng build ngx-chessground --configuration production",
    "build:app:prod": "ng build ngx-chessground-example --configuration production --output-path docs --base-href /ngx-chessground/",
    "test": "ng test",
    "lint": "ng lint ngx-chessground --format=visualstudio",
    "lint:fix": "ng lint ngx-chessground --fix",
    "e2e": "ng e2e",
    "format": "npx prettier --write .",
    "compodoc": "npx compodoc -p projects/ngx-chessground/tsconfig.doc.json -s -r 9090 -o"
  },
  "dependencies": {
    "@angular/animations": "14.0.0",
    "@angular/cdk": "14.0.0",
    "@angular/common": "14.0.0",
    "@angular/compiler": "14.0.0",
    "@angular/core": "14.0.0",
    "@angular/forms": "14.0.0",
    "@angular/material": "^14.0.0",
    "@angular/platform-browser": "14.0.0",
    "@angular/platform-browser-dynamic": "14.0.0",
    "@angular/router": "14.0.0",
    "chess.js": "0.13.2",
    "chessground": "8.2.1",
    "rxjs": "7.5.5",
    "snabbdom": "3.5.0",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.0",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "14.0.0",
    "@angular/compiler-cli": "14.0.0",
    "@compodoc/compodoc": "1.1.19",
    "@types/chess.js": "0.13.1",
    "@types/jasmine": "4.0.3",
    "@types/node": "17.0.40",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-markdown": "2.2.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prefer-arrow": "latest",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "latest",
    "jasmine-core": "4.1.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.20",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.0.1",
    "karma-jasmine-html-reporter": "2.0.0",
    "ng-packagr": "14.0.0",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "protractor": "7.0.0",
    "ts-node": "10.8.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/topce/ngx-chessground.git"
  },
  "keywords": [
    "angular",
    "chess",
    "pgn",
    "fen",
    "chessground",
    "chess.js",
    "lichess"
  ],
  "author": "topce",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/topce/ngx-chessground/issues"
  },
  "homepage": "https://github.com/topce/ngx-chessground#readme"
}