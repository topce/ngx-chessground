{
  "name": "ngx-chessground",
  "version": "16.0.3",
  "description": "angular wrapper for ornicar/chessground",
  "scripts": {
    "postinstall": "patch-package",
    "tsc-lib": "tsc --project ./projects/ngx-chessground/tsconfig.lib.json",
    "tsc-app": "tsc --project ./projects/ngx-chessground-example/tsconfig.app.json",
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "build:lib:prod": "ng build ngx-chessground --configuration production",
    "build:app:prod": "ng build ngx-chessground-example --configuration production --output-path docs --base-href /ngx-chessground/",
    "test": "ng test",
    "check": "npx @biomejs/biome check --write ./projects ",
    "lint": "npx @biomejs/biome lint ./projects",
    "lint:fix": "npx @biomejs/biome lint --write ./projects",
    "e2e": "ng e2e",
    "format": "npx @biomejs/biome format --write ./projects",
    "compodoc": "npx compodoc -p projects/ngx-chessground/tsconfig.doc.json -s -r 9090 -o"
  },
  "dependencies": {
    "@angular/animations": "19.0.5",
    "@angular/cdk": "19.0.4",
    "@angular/common": "19.0.5",
    "@angular/compiler": "19.0.5",
    "@angular/core": "19.0.5",
    "@angular/forms": "19.0.5",
    "@angular/material": "19.0.4",
    "@angular/platform-browser": "19.0.5",
    "@angular/platform-browser-dynamic": "19.0.5",
    "@angular/router": "19.0.5",
    "chess.js": "0.13.4",
    "chessground": "9.1.1",
    "rxjs": "7.8.1",
    "snabbdom": "3.6.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.0.6",
    "@angular-eslint/builder": "19.0.2",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/schematics": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/cli": "19.0.6",
    "@angular/compiler-cli": "19.0.5",
    "@biomejs/biome": "1.9.4",
    "@compodoc/compodoc": "1.1.26",
    "@types/chess.js": "0.13.7",
    "@types/jasmine": "5.1.5",
    "@types/node": "22.10.2",
    "jasmine-core": "5.5.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ng-packagr": "19.0.1",
    "patch-package": "^8.0.0",
    "prettier": "3.4.2",
    "pretty-quick": "4.0.0",
    "protractor": "7.0.0",
    "typescript": "5.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/topce/ngx-chessground.git"
  },
  "keywords": [
    "angular",
    "chess",
    "pgn",
    "fen",
    "chessground",
    "chess.js",
    "lichess"
  ],
  "author": "topce",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/topce/ngx-chessground/issues"
  },
  "homepage": "https://github.com/topce/ngx-chessground#readme"
}
