{
  "name": "ngx-chessground",
  "version": "12.2.1",
  "description": "angular wrapper for ornicar/chessground",
  "scripts": {
    "tsc-lib": "tsc --project ./projects/ngx-chessground/tsconfig.lib.json",
    "tsc-app": "tsc --project ./projects/ngx-chessground-example/tsconfig.app.json",
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "build:lib:prod": "ng build ngx-chessground --configuration production",
    "build:app:prod": "ng build ngx-chessground-example --configuration production --output-path docs --base-href /ngx-chessground/",
    "test": "ng test",
    "lint": "ng lint ngx-chessground --format=visualstudio",
    "lint:fix": "ng lint ngx-chessground --fix",
    "e2e": "ng e2e",
    "format": "npx prettier --write .",
    "compodoc": "npx compodoc -p projects/ngx-chessground/tsconfig.doc.json -s -r 9090 -o"
  },
  "dependencies": {
    "@angular/animations": "12.2.0",
    "@angular/cdk": "12.2.0",
    "@angular/common": "12.2.0",
    "@angular/compiler": "12.2.0",
    "@angular/core": "12.2.0",
    "@angular/forms": "12.2.0",
    "@angular/material": "^12.2.0",
    "@angular/platform-browser": "12.2.0",
    "@angular/platform-browser-dynamic": "12.2.0",
    "@angular/router": "12.2.0",
    "chess.js": "0.12.0",
    "chessground": "8.1.5",
    "rxjs": "7.3.0",
    "snabbdom": "3.0.3",
    "tslib": "2.3.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.0",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/schematics": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "12.2.0",
    "@angular/compiler-cli": "12.2.0",
    "@compodoc/compodoc": "1.1.14",
    "@types/chess.js": "0.11.2",
    "@types/jasmine": "3.8.2",
    "@types/node": "16.4.13",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-markdown": "2.2.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prefer-arrow": "latest",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "latest",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "ng-packagr": "12.2.0",
    "prettier": "2.3.2",
    "pretty-quick": "3.1.1",
    "protractor": "7.0.0",
    "ts-node": "10.1.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/topce/ngx-chessground.git"
  },
  "keywords": [
    "angular",
    "chess",
    "pgn",
    "fen",
    "chessground",
    "chess.js",
    "lichess"
  ],
  "author": "topce",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/topce/ngx-chessground/issues"
  },
  "homepage": "https://github.com/topce/ngx-chessground#readme"
}
